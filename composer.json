{
  "name": "hofff/contao-restrict-get-parameters",
  "description": "Restrict allowed get parameters",
  "license": "MIT",
  "type": "contao-bundle",
  "authors": [
    {
      "name": "David Molineus",
      "email": "david@hofff.com"
    }
  ],
  "require": {
    "php": "^8.1",
    "contao/core-bundle": "^4.13",
    "symfony/config": "^5.4",
    "symfony/dependency-injection": "^5.4",
    "symfony/http-foundation": "^5.4",
    "symfony/http-kernel": "^5.4"
  },
  "require-dev": {
    "contao/manager-plugin": "^2.1",
    "doctrine/coding-standard": "^12.0",
    "phpcq/runner-bootstrap": "^1.0@dev"
  },
  "autoload": {
    "psr-4": {
      "Hofff\\Contao\\RestrictGetParameters\\": "src/"
    }
  },
  "config": {
    "allow-plugins": {
      "contao-components/installer": true,
      "php-http/discovery": true,
      "contao/manager-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    },
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.0.x-dev"
    },
    "contao-manager-plugin": "Hofff\\Contao\\RestrictGetParameters\\ContaoManager\\Plugin"
  }
}
